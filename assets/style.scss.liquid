  .variant-select__list {
    width: 100%;
    opacity: 0;
    visibility: hidden;
    position: absolute;
    z-index: 10;
    bottom: 90%;
    left: 0;
    right: 0;
    background: white;
    -ms-transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
    transition: all 0.3s ease-out;
  }
  .variant-select__option {
   display: flex;
    a {
    font-weight:normal;
    padding: 8px 10px 0;
    display: -webkit-flex;
    display: -ms-flexbox;
    display: flex;
    width: 100%;
    -ms-flex-align: center;
    -webkit-align-items: center;
    -moz-align-items: center;
    -ms-align-items: center;
    -o-align-items: center;
    align-items: center;
    }
    &.active {
    display:none;
    }
  }
   .variant-select__option-label {
    padding: 8px 10px;
    font-size: 0.875em;
     @include bp(mobile) {
         font-size: 0.99em;
     }
  }
  .color-select__image {
    background-position: 50%;
    border: 1px solid #e5e7e7;
    background-size: cover;
    background-repeat: no-repeat;
    overflow: hidden;
    width: 40px;
    height: 30px;
    @include bp(mobile) {
        width: 16px;
    height: 15px;
    }
   }
   &.is-showing {
    .variant-select__list {
     bottom: 100%;
     opacity: 1;
     visibility: visible;
    }
    .variant-select__selected {
     &:after {
    -ms-transform: translateY(-50%) rotate(-180deg);
    -webkit-transform: translateY(-50%) rotate(-180deg);
    transform: translateY(-50%) rotate(-180deg);
     } 
    }
