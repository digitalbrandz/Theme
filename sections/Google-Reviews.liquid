{% style %}
{% assign loadedfonts = '' %}    
{% assign familyandweight = section.settings.font.family | append: section.settings.font.weight %}
{% if loadedfonts contains familyandweight %}
{% else %}
  {{ section.settings.font | font_face }}  
{% endif %}   
{% assign loadedfonts = loadedfonts | append: section.settings.font.family | append: section.settings.font.weight | append: section.settings.font.style | append: '|' %}


 #shopify-section-{{ section.id }} .google-reviews-wrap > .title h2 {
    font-family: {{ section.settings.font.family }}; 
    font-weight:{{ section.settings.font.weight }}; 
    font-style:{{ section.settings.font.style }};    
      font-size:{{ section.settings.fontsize }}px;   
 }



   #shopify-section-{{ section.id }} .google-reviews-wrap > .title {
     text-align: {{ section.settings.alignment }}; 
   }
  
{% if section.settings.linethrough %}
   #shopify-section-{{ section.id }} .google-reviews-wrap > .title h2:before {  
    position: absolute;
    content: "";
    height: 1px;
    background-color: #e0e0e0;
    top: -moz-calc(50% - 0.5px);
    top: -webkit-calc(50% - 0.5px);
    top: -ms-calc(50% - 0.5px);
    top: calc(50% - 0.5px);
    left: 0;
    right: 0;
    width: 100%;
   }
  
{% endif %}
{% endstyle %}

<div class="google-reviews-wrap">
   
<div class="title">
  <div class="column">
    {% unless section.settings.title == nil %}
    <h2><span>{{ section.settings.title }}</span></h2>
    {% endunless %}
  </div>
</div>
  
  
   <div>
      <!-- Slider main container -->
      <div class="swiper-container google-swiper" id="google-reviews-{{ section.id }}">
         <!-- Additional required wrapper -->
        <div class="swiper-wrapper">
            <!-- Slides -->
        

      {% assign i = 0 %}
       {% for block in section.blocks %}
          {% case block.type %}
             {% when 'Review' %}
      
      <div class="swiper-slide">
