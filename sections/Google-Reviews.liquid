<script>
var callbackGoogle_Reviews = function(){
  // Handler when the DOM is fully loaded
  if(!window.Swiper) {
    // add script
    var script = document.createElement("script");
    script.type = "text/javascript";   
    script.src = 'https://cdn.jsdelivr.net/npm/swiper@5.2.1/js/swiper.min.js';
    document.getElementsByTagName("head")[0].appendChild(script);
    // add style
    var style = document.createElement("link");
    style.type = "text/css";  
    style.rel = "stylesheet";
    style.href = 'https://cdn.jsdelivr.net/npm/swiper@5.2.1/css/swiper.min.css';
    document.getElementsByTagName("head")[0].appendChild(style);
  }
};

if (
    document.readyState === "complete" ||
    (document.readyState !== "loading" && !document.documentElement.doScroll)
) {
  callbackGoogle_Reviews();
} else {
  document.addEventListener("DOMContentLoaded", callbackGoogle_Reviews);
}
    
  
  
  
  
function deferGoogle_Reviews(methodGoogle_Reviews) {
    if (window.jQuery && window.Swiper) {
        methodGoogle_Reviews();
    } else {
        setTimeout(function() { deferGoogle_Reviews(methodGoogle_Reviews) }, 50);
    }
}
deferGoogle_Reviews(function () {  
  
  startGoogleSlider();
  
  
function startGoogleSlider() {
   
      var container = document.getElementById('google-reviews-{{ section.id }}');
  
      var swiper = new Swiper((container), {         
     	effect: 'slide',
		parallax: true,
		loop: true,
		speed: 600,
		slidesPerView: 3,
		slidesPerGroup: 1,
		autoplay: {
			delay: 7000,
			disableOnInteraction: true,
			observeParents: true,
		},
		breakpoints: {
			// when window width is <= 320px
          	1022: {
				// speed: 300,
                slidesPerView: 3,
                slidesPerGroup: 1,
				autoHeight: true,
			},
			300: {
				// speed: 300,
                slidesPerView: 2,
                slidesPerGroup: 2,
				autoHeight: true,
			}
		}
      });   
} 
    
  
  
  
});  
</script>

